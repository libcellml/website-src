        <argsstring>(const std::string &amp;input, bool parseVersion1XModels=false)</argsstring>
        <name>parseModel</name>
        <qualifiedname>libcellml::Parser::parseModel</qualifiedname>
        <param>
          <type>const std::string &amp;</type>
          <declname>input</declname>
        </param>
        <param>
          <type>bool</type>
          <declname>parseVersion1XModels</declname>
          <defval>false</defval>
